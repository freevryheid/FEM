FC = g77                     # FORTRAN COMPILER
FFLAGS = -g -fbounds-check -fugly-assumed   # ONLY UNCOMMENT FOR DEVELOPMENT PURPOSES
#FFLAGS = -O3                # ONLY UNCOMMENT FOR FINAL COMPILATION (FOR SPEED)

OBJS = ASEULR.O ASFG.O ASGRAD.O ASKG.O ASKGD.O ASMG.O ASNEWT.O ASRESD.O ASSEL.O ASSELD.O B02.O BASEP.O BLCOMT.O BLCOND.O BLCOOR.O BLDLPN.O BLELEM.O BLIMAG.O \
            BLLIND.O BLLINM.O BLNLIN.O BLPREL.O BLPRND.O BLSOLC.O BLSOLR.O BLTEMP.O BLVALP.O BTDB.O D02.O DATA.O DLELM.O DNIDX.O ELEM01.O ELEM02.O ELEMLB.O EQBLOC.O \
            ERREUR.O ESPACE.O EULER.O EXCOND.O EXCOOR.O EXDLPN.O EXELEM.O EXLIND.O EXLINM.O EXNLIN.O EXPREL.O EXPRND.O EXSOLC.O EXSOLR.O EXTEMP.O EXVALP.O FEM.O \
            GAUSS.O INIT.O INVERS.O JACOB.O JACOBI.O LOCELD.O MAJ.O MODFG.O MULKU.O NEWTON.O NI.O NI01.O NI02.O NORME.O PNINV.O PRELEM.O PRPVTS.O PRRESD.O PRSOL.O \
            RDELEM.O SCAL.O SOL.O SOLD.O VIDE.O WRELEM.O XTRELM.O

FEM: $(OBJS)	
	$(FC) -o FEM $(FFLAGS) $(OBJS) 
